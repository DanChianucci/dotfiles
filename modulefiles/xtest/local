#%Module
proc ModulesHelp { } {
    global ver

    puts stderr "Xelic Test"
    puts stderr "\nVersion ${ver}\n"
}

prereq python

# for Tcl script use only
set     ver          "local"
set     basepath     "/home/chianucci/Workareas/tools/xtest2"

module-whatis   "XTEST ${ver}"
prepend-path PATH ${basepath}
