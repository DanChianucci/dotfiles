#%Module
conflict xregmap

if { ! [is-loaded python] } {
  module load python
}

if { ! [is-loaded texlive] } {
  module load texlive
}

prereq python
prereq texlive

#Tcl script use only
set ver          "local"
set xregmap_root  "/home/chianucci/Workareas/tools/xregmap"
set xregmap_bin   "${xregmap_root}/xregmap"

module-whatis   "XREGMAP ${ver}"


prepend-path PATH ${xregmap_bin}
setenv XREGMAP_ROOT ${xregmap_root}
setenv XREGMAP_BIN ${xregmap_bin}
setenv TEXINPUTS .:${xregmap_bin}/texmf//:

set-alias iplint "xmllint --noout --schema /home/projects/xelic/tools/xregmap/snapshots/ieee-1685-2014/index.xsd"
